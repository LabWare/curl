Subject: [PATCH] Disable actionlint step in checksrc workflow

---
 .github/workflows/checksrc.yml | 1 +
 1 file changed, 1 insertion(+)

diff --git a/.github/workflows/checksrc.yml b/.github/workflows/checksrc.yml
index 90bd62f19c..b0e585e10e 100644
--- a/.github/workflows/checksrc.yml
+++ b/.github/workflows/checksrc.yml
@@ -154,6 +154,7 @@ jobs:
           zizmor --pedantic .github/workflows/*.yml .github/dependabot.yml
 
       - name: 'actionlint'
+        if: false
         run: |
           eval "$(/home/linuxbrew/.linuxbrew/bin/brew shellenv)"
           export SHELLCHECK_OPTS='--exclude=1090,1091,2086,2153 --enable=avoid-nullary-conditions,deprecate-which'
